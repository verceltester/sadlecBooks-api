{
  "builds": [{
      "src": "restapi/wsgi.py",
      "use": "@vercel/python",    
      "config": { "maxLambdaSize": "30mb", "runtime": "python3.9" }
  }],
  "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "restapi/wsgi.py"
      }
  ]
}
